{
  "name": "HalfScroll",
  "version": "0.0.2",
  "description": "Scroll only half a page with space key",
  "offline_enabled": true,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_matches":["*://aristidesfl.github.com/smoothkeyscroll*"],
      "js": ["half.js"]
    }
  ],
  "permissions": [
     "storage"
   ],
  "manifest_version": 2,
  "homepage_url": "http://hayamiz.com/"
}
